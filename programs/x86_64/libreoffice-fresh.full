#!/bin/sh

APP=libreoffice
URL="https://libreoffice.soluzioniopen.com/stable/full/LibreOffice-fresh.full-x86_64.AppImage"

mkdir /opt/libreoffice
cd /opt/libreoffice

echo '#!/bin/sh' >> /opt/libreoffice/remove
echo "rm -R -f /usr/share/applications/$APP* /opt/libreoffice /usr/local/bin/$APP" >> /opt/libreoffice/remove
chmod a+x /opt/libreoffice/remove

mkdir tmp;
cd ./tmp;
wget $URL;
wget $URL.asc;
wget $URL.zsync;
wget $URL.zsync.asc;
cd ..
mv ./tmp/*mage ./$APP
chmod a+x /opt/libreoffice/$APP
mv ./tmp/*mage.zsync ./$APP.zsync
mv ./tmp/*mage.asc ./$APP.asc
mv ./tmp/*zsync.asc ./$APP.zsync.asc
rmdir ./tmp

# LINK
ln -s /opt/$APP/$APP /usr/local/bin/$APP

# SCRIPT TO UPDATE THE PROGRAM
echo '#!/bin/sh' >> /opt/$APP/AM-updater
echo "cd /opt/libreoffice
mkdir tmp
cd ./tmp
wget $URL.asc;
wget $URL.zsync;
wget $URL.zsync.asc
cd ..
mv ./tmp/*mage.zsync ./$APP.zsync
mv ./tmp/*mage.asc ./$APP.asc
mv ./tmp/*zsync.asc ./$APP.zsync.asc
rmdir ./tmp
zsync /opt/$APP/$APP.zsync && rm -R -f /opt/$APP/*zs-old /opt/$APP/*.part
chmod a+x /opt/$APP/$APP" >> /opt/$APP/AM-updater

chmod a+x /opt/$APP/AM-updater

echo "#
